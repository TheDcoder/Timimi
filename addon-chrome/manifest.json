{

    "description": "Addon to save Tiddlywiki",
    "manifest_version": 2,
    "name": "timimi",
    "version": "2.0.3",
    "icons":
    {
        "128": "icons/fish.png"
    },
    "background":
    {
        "scripts": ["background.js"],
	"persistent": true
    },
    "browser_action":
    {
        "default_icon": "icons/fish.png"
    },
    "content_scripts": [
    {
        "matches": ["file:///*"],
        "js": ["content-script.js"]
    }],
    "options_ui": { 
         "page": "settings/options.html" 
     },
    "permissions": [
        "nativeMessaging",
        "storage"
    ],
    "key": "83b0503ca80442e0a03dd186c3a9020923322158911a46a88d8ea668da086ee4"
}
